<!DOCTYPE html>
<html lang="pl">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Przekierowanie</title>
    <script type="text/javascript">
        // Lista linków do rotacji
        const links = [
            "https://sites.google.com/view/navalseductionstrategies/home",
            "https://www.omegle1.com/",
            "https://www.vigrxplus.com/ct/944321?t1=urlrotator",
            "https://sites.google.com/view/bioleanclickbank/home"
        ];

        // Funkcja losująca link
        function redirectRandomly() {
            const randomIndex = Math.floor(Math.random() * links.length); // Losuje indeks
            window.location.href = links[randomIndex]; // Przekierowuje użytkownika
        }

        // Przekierowanie po załadowaniu strony
        window.onload = redirectRandomly;
    </script>
</head>
<body>
    <p>Trwa przekierowanie...</p>
</body>
</html>
